<?xml version="1.0" encoding="UTF-8"?>
<site xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd"
      xmlns="http://maven.apache.org/SITE/2.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      name="${this.name} ${this.version}">
    <publishDate format="dd-MMMM-yyyy"/>
    <version/>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>${maven-fluido-skin.version}</version>
    </skin>
    <bannerLeft name="by ${this.organization.name}" href="${this.organization.url}">
        <image src="images/logo.png" alt=""/>
    </bannerLeft>
    <bannerRight name="${project.name}" href="${project.url}"/>
    <body>
        <head/>
        <links>
            <item name="Issues dashboard" href="${this.issueManagement.url}"/>
        </links>
        <menu name="Information">
            <item name="Home" href="/index.html"/>
        </menu>
        <menu ref="reports"/>
    </body>
    <poweredBy>
        <logo href="https://github.com/Tailormap/tailormap-api/actions/workflows/ubuntu-maven.yml"
              name="Maven CI Build status (main branch)">
            <image src="https://github.com/Tailormap/tailormap-api/actions/workflows/ubuntu-maven.yml/badge.svg"
                   alt=""/>
        </logo>
    </poweredBy>
</site>
